{
  "name": "typescript-backend",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "generate:doc": "swagger-cli bundle docs/openapi.yaml --outfile _build/openapi.yaml --type yaml",
    "clean": "rm -rf ./dist",
    "build": "tsc",
    "lint": "eslint --ext .ts src/",
    "start": "node ./dist/entrypoints/api.js",
    "serve": "tsc-watch --onSuccess \"node -r dotenv/config ./dist/entrypoints/api.js\"",
    "test": "DOTENV_CONFIG_PATH=.env.test jest --setupFiles dotenv/config",
    "test:pipeline": "DOTENV_CONFIG_PATH=.env.test jest --verbose --runInBand --setupFiles dotenv/config",
    "test:watch": "DOTENV_CONFIG_PATH=.env.test jest --verbose --setupFiles dotenv/config --watch",
    "test:coverage": "DOTENV_CONFIG_PATH=.env.test jest --verbose --setupFiles dotenv/config --coverage",
    "serve:test": "DOTENV_CONFIG_PATH=.env.test tsc-watch --onSuccess \"node --trace-deprecation -r dotenv/config ./dist/entrypoints/api.js\"",
    "typeorm": "ts-node -r dotenv/config ./node_modules/typeorm/cli -d ./src/config/sql.ts",
    "mjml": "./node_modules/.bin/mjml templates/mjml/*.mjml -o templates/html/",
    "prep:push": "pnpm clean && pnpm lint && pnpm build && pnpm generate:doc && npx madge --circular --extensions ts src/"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@appwise/express-dto-router": "^0.0.30",
    "@appwise/oauth2-server": "^0.0.10",
    "@sentry/node": "^7.37.1",
    "@sentry/tracing": "^7.37.1",
    "axios": "^1.3.2",
    "bcryptjs": "^2.4.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "cors": "^2.8.5",
    "ejs": "^3.1.8",
    "express": "^4.18.2",
    "express-basic-auth": "^1.2.1",
    "form-data": "^4.0.0",
    "jose": "^4.11.4",
    "jsonwebtoken": "^9.0.0",
    "mailgun.js": "^8.0.6",
    "mjml": "^4.13.0",
    "pg": "^8.9.0",
    "reflect-metadata": "^0.1.13",
    "swagger-cli": "^4.0.4",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^4.6.0",
    "typeorm": "^0.3.12",
    "uuid": "^9.0.0",
    "validator": "^13.9.0",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "@ngneat/falso": "^6.4.0",
    "@sentry/types": "^7.37.1",
    "@types/bcryptjs": "^2.4.2",
    "@types/cors": "^2.8.13",
    "@types/ejs": "^3.1.1",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.4.0",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/node": "^18.13.0",
    "@types/pg": "^8.6.6",
    "@types/supertest": "^2.0.12",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/validator": "^13.7.12",
    "@types/yamljs": "^0.2.31",
    "@typescript-eslint/eslint-plugin": "^5.51.0",
    "@typescript-eslint/parser": "^5.51.0",
    "dotenv": "^16.0.3",
    "eslint": "^8.33.0",
    "eslint-config-appwise": "^1.0.9",
    "eslint-config-standard": "^17.0.0",
    "eslint-config-standard-with-typescript": "^34.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-putout": "^16.3.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "expect": "^29.4.2",
    "jest": "^29.4.2",
    "putout": "^28.16.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
